    type = RunException
    input = require_stochastic_results.i
    expect_err = "The 'results' object must be a 'StochasticResults' object."
    requirement = "MOOSE shall produce an error if the 'result' object in "
                  "'SamplerPostprocessorTransfer' is not a 'StochasticResults object'."
